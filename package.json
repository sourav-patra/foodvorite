{
  "name": "foodvorite",
  "version": "1.0.0",
  "description": "personal favorite dish tracker",
  "private": true,
  "scripts": {
    "sass-compile": "node-sass src/styles/scss -o src/styles/css",
    "sass-watch": "node-sass -w src/styles/scss -o src/styles/css",
    "live": "live-server",
    "test": "echo \"Error: no test specified\" && exit 1",
    "serve": "run-p sass-watch live ",
    "serve:dist": "run-s sass-compile build",
    "build": "webpack --config webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sourav-patra/foodvorite.git"
  },
  "keywords": [
    "favorite",
    "food",
    "favorite",
    "dish",
    "food",
    "tracker",
    "dish",
    "tracker",
    "food",
    "order"
  ],
  "author": "Sourav Patra",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sourav-patra/foodvorite/issues"
  },
  "homepage": "https://github.com/sourav-patra/foodvorite#readme",
  "dependencies": {
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "require": "^2.4.20",
    "webpack": "^5.11.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.2",
    "babel-preset-es2015": "^6.24.1",
    "css-loader": "^5.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^6.2.0",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^4.5.0",
    "live-server": "^1.2.1",
    "node-sass": "^5.0.0",
    "sass-loader": "^10.1.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  }
}
